<%= form_for(@restaurant) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_area :address %>
    <%= f.label :category %>
    <%= f.text_area :category %>
    <%= f.label :phone_number %>
    <%= f.text_area :phone_number %>
  <%= f.submit %>
<% end %>
